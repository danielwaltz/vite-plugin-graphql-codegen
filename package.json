{
  "name": "vite-plugin-graphql-codegen",
  "version": "3.0.0",
  "license": "MIT",
  "author": "Daniel Waltz",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielwaltz/vite-plugin-graphql-codegen.git"
  },
  "keywords": [
    "graphql",
    "codegen",
    "graphql-codegen",
    "graphql-code-generator",
    "vite",
    "vite-plugin",
    "vite-plugin-graphql-codegen"
  ],
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint . --no-fix --ignore-path .gitignore --max-warnings 0",
    "type-check": "tsc --noEmit",
    "release": "standard-version",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "peerDependencies": {
    "@graphql-codegen/cli": "^1.0.0 || ^2.0.0",
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "vite": "^2.7.0 || ^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^16.18.8",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "prettier": "^2.8.1",
    "semantic-release": "^19.0.5",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  }
}
